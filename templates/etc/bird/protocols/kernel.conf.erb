#Managed by puppet
protocol kernel <%= @name %> {
<%- if @table -%>
  table <%= @table %>;
<%- end -%>
<%- if @debug == 'all' -%>
  debug all;
<%- elsif @debug != 'off' and @debug -%>
  debug { <%= @debug -%> };
<%- end -%>
<%- if @learn -%>
  learn;
<%- end -%>
<%- if @persist -%>
  persist;
<%- end -%>
<%- if @scan_time -%>
  scan time <%= @scan_time %>;
<%- end -%>
  import <%= @import_filter %>;
  export <%= @export_filter %>;
}
