#Managed by puppet
protocol device <%= @name %> {
<%- if @table -%>
  table <%= @table %>;
<%- end -%>
<%- if @debug == 'all' -%>
  debug all;
<%- elsif @debug != 'off' -%>
  debug { <%= @debug -%> };
<%- end -%>
<%- if @scan_time -%>
  scan time <%= @scan_time %>;
<%- end -%>
<%- if @primary -%>
  primary <%= @primary %>;
<%- end -%>
}
