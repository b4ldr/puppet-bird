<%- if @table -%>
  table <%= @table %>;
<%- end -%>
<%- if ['off','all'].include?(@debug) -%>
  debug <%= @debug -%>;
<%- else -%>
  debug { <%= @debug -%> };
<%- end -%>
<%- if @multihop -%>
  multihop <%= @multihop %>;
<%- end -%>
<%- if @next_hop_self -%>
  next hop self;
<%- end -%>
<%- if @next_hop_keep -%>
  next hop keep;
<%- end -%>
<%- if @missing_lladdr -%>
  missing lladdr <%= @missing_lladdr %>;
<%- end -%>
<%- if @gateway -%>
  missing lladdr <%= @gateway %>;
<%- end -%>
<%- if @ttl_security -%>
  ttl security on;
<%- end -%>
<%- if @password -%>
  password <%= @password %>;
<%- end -%>
<%- if @rs_client -%>
  rs client;
<%- end -%>
<%- if @import_filter -%>
  import <%= @import_filter %>;
<%- end -%>
<%- if @export_filter -%>
  export <%= @export_filter %>;
<%- end -%>
